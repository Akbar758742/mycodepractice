

#include<stdio.h> 

int main()
{
    float marks;
    char grade;

    printf("Enter marks: ");
    scanf("%f", &marks);

    if(marks >= 80)
    {
        grade = 'A+';
    }
    else if(marks >= 70 && marks < 80)
    {
        grade = 'A';
    }
    else if(marks >= 60 && marks < 70)
    {
        grade = 'A-';
    }
    else if(marks >= 50 && marks < 60)
    {
        grade = 'B';
    }
    else if(marks >= 33 && marks < 50)
    {
        grade = 'E';
    }
    else
    {
        grade = 'F';
    }

    printf("Your grade is %c", grade);

    return 0;
}
